    "\n",
    "with open(\"example_local_smbl.xml\", \"r\", encoding=\"utf-8\") as file:\n",
    "    xml_string = file.read()\n",
    "\n",
    "model = loads(xml_string, name=\"CarbonMetabolism\")\n",
